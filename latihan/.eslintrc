{
    "extends": [
      "wesbos"
    ],
    "settings": {
        "import/resolver": {
            "node": {
                "paths": ["src", "src/assets"]
            }
        }
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 120,
          "tabWidth": 2
        }
      ],
      "import/no-extraneous-dependencies": "off",
      "import/named": "off",
      "react/prop-types": [
        "error",
        {
          "ignore": ["navigation", "screenProps"]
        }
      ],
      "no-console": [
        "warn",
        {
          "allow": ["warn","error"]
        }
      ]
    }
}